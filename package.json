{
  "name": "prototyped.js",
  "version": "0.4.0",
  "description": "Common typescript ready prototypes available in both es5 and es6",
  "author": "Ardalan Amini <ardalanamini22@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/ardalanamini/prototyped.js#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ardalanamini/prototyped.js.git"
  },
  "bugs": {
    "url": "https://github.com/ardalanamini/prototyped.js/issues"
  },
  "keywords": [
    "server-side",
    "client-side",
    "common",
    "prototype",
    "array",
    "all",
    "any",
    "isInstance",
    "range",
    "repeat",
    "avg",
    "first",
    "last",
    "initial",
    "tail",
    "chunk",
    "compact",
    "count",
    "countBy",
    "flatten",
    "deepFlatten",
    "diff",
    "distinct",
    "everyNth",
    "groupBy",
    "indexOfAll",
    "intersect",
    "partition",
    "pull",
    "sample",
    "shuffle",
    "union",
    "zip",
    "zipObject",
    "pluck",
    "sum",
    "average",
    "max",
    "min",
    "contains",
    "crossJoin",
    "get",
    "implode",
    "clone",
    "median",
    "pad",
    "append",
    "prepend",
    "boolean",
    "date",
    "function",
    "once",
    "defer",
    "cache",
    "string",
    "chars",
    "humanize",
    "swapCase",
    "capitalize",
    "decapitalize",
    "mask",
    "pluralize",
    "reverse",
    "lines",
    "camelCase",
    "kebabCase",
    "snakeCase",
    "truncate",
    "words",
    "math",
    "factorial",
    "fibonacci",
    "gcd",
    "lcm",
    "isEven",
    "isPrime",
    "number",
    "object",
    "invert",
    "lowerCaseKeys",
    "map",
    "mapKeys",
    "mergeKeys",
    "merge",
    "size"
  ],
  "main": "dist/index.js",
  "files": [
    "dist",
    "es6"
  ],
  "scripts": {
    "doc": "tsc && jsdoc2md es6/*.js es6/**/*.js es6/**/**/*.js --example-lang javascript > API.md",
    "build": "node scripts/build.js",
    "prepare": "npm run build && npm test",
    "lint": "tslint -p tsconfig.json -c tslint.json --exclude '**/*.d.ts'",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "^9.6.4",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "fs-extra": "^5.0.0",
    "fs-readdir-recursive": "^1.1.0",
    "jest": "^22.4.3",
    "jsdoc-to-markdown": "^3.0.4",
    "rimraf": "^2.6.2",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1",
    "uglify-es": "^3.3.10",
    "uglify-js": "^3.3.21"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "<rootDir>/scripts/preprocessor.js"
    },
    "testMatch": [
      "<rootDir>/lib/**/test\\.(ts|js)"
    ]
  }
}
