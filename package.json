{
  "name": "prototyped.js",
  "version": "0.12.0",
  "description": "Common typescript ready prototypes available in both es5 and es6",
  "author": "Ardalan Amini <ardalanamini22@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/ardalanamini/prototyped.js#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ardalanamini/prototyped.js.git"
  },
  "bugs": {
    "url": "https://github.com/ardalanamini/prototyped.js/issues"
  },
  "keywords": [
    "server-side",
    "client-side",
    "common",
    "prototype",
    "array",
    "all",
    "any",
    "isInstance",
    "range",
    "repeat",
    "avg",
    "first",
    "last",
    "initial",
    "tail",
    "chunk",
    "compact",
    "count",
    "countBy",
    "flatten",
    "deepFlatten",
    "diff",
    "distinct",
    "everyNth",
    "groupBy",
    "indexOfAll",
    "intersect",
    "partition",
    "pull",
    "sample",
    "shuffle",
    "skip",
    "offset",
    "union",
    "where",
    "whereBetween",
    "whereIn",
    "whereLike",
    "whereNotBetween",
    "whereNotIn",
    "whereNotLike",
    "whereNotNull",
    "whereNull",
    "zip",
    "zipObject",
    "pluck",
    "sum",
    "average",
    "max",
    "min",
    "contains",
    "crossJoin",
    "get",
    "implode",
    "clone",
    "median",
    "pad",
    "append",
    "prepend",
    "boolean",
    "date",
    "function",
    "once",
    "defer",
    "cache",
    "string",
    "chars",
    "humanize",
    "swapCase",
    "capitalize",
    "decapitalize",
    "mask",
    "pluralize",
    "reverse",
    "singularize",
    "lines",
    "camelCase",
    "kebabCase",
    "snakeCase",
    "truncate",
    "words",
    "math",
    "factorial",
    "fibonacci",
    "gcd",
    "lcm",
    "isEven",
    "isPrime",
    "number",
    "object",
    "invert",
    "lowerCaseKeys",
    "map",
    "mapKeys",
    "mergeKeys",
    "merge",
    "size"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "files": [
    "dist",
    "es6"
  ],
  "scripts": {
    "prepublishOnly": "npm test && npm run lint && npm run build && npm run doc",
    "build": "node scripts/build.js",
    "doc": "tsc && jsdoc2md es6/*.js es6/**/*.js es6/**/**/*.js --example-lang javascript > API.md",
    "lint": "tslint -p tsconfig.json -c tslint.json --exclude '**/*.d.ts'",
    "test": "jest",
    "coverage": "npm test -- --coverage",
    "codecov": "npm run coverage && codecov"
  },
  "dependencies": {
    "pluralize": "^7.0.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.0",
    "@types/node": "^10.5.3",
    "@types/pluralize": "0.0.29",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "codecov": "^3.0.4",
    "fs-extra": "^7.0.0",
    "fs-readdir-recursive": "^1.1.0",
    "jest": "^23.4.1",
    "jsdoc-to-markdown": "^4.0.1",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.0.1",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2",
    "uglify-es": "^3.3.9",
    "uglify-js": "^3.4.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testMatch": [
      "<rootDir>/src/**/test\\.ts",
      "<rootDir>/src/**/*\\.test\\.ts"
    ]
  }
}
