  "scripts": {
    "prepublishOnly": "npm test && npm run lint && npm run doc && npm run build && npm run regenerate:package.json",
    "build": "node scripts/build.js",
    "doc": "node scripts/doc.js",
    "lint": "tslint -p tsconfig.json -c tslint.json --exclude '**/*.d.ts'",
    "test": "jest",
    "coverage": "npm test -- --coverage",
    "codecov": "npm run coverage -- --runInBand && codecov",
    "regenerate:package.json": "node scripts/regenerate-package-json.js"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.2",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "codecov": "^3.5.0",
    "fs-extra": "^8.0.1",
    "fs-readdir-recursive": "^1.1.0",
    "jest": "^24.8.0",
    "jsdoc-to-markdown": "^5.0.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5",
    "uglify-es": "^3.3.9",
    "uglify-js": "^3.5.15"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testMatch": [
      "<rootDir>/src/**/test\\.ts",
      "<rootDir>/src/**/*\\.test\\.ts"
    ]
  },
  "pre-commit": "prepublishOnly"
}
